{
  "name": "springify",
  "version": "1.0.2",
  "description": "A tiny lil' (under 1kb gzip) utility function for creating dynamic spring animations",
  "author": "Luke Carl Thompson <hello@lukethompson.com.au> (https://lukethompson.com.au/)",
  "license": "MIT",
  "keywords": [
    "spring",
    "bounce",
    "animation"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LukeCarlThompson/springify.git"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/Springify.umd.js",
  "module": "./dist/Springify.es.js",
  "exports": {
    ".": {
      "import": "./dist/Springify.es.js",
      "require": "./dist/Springify.umd.js"
    }
  },
  "private": false,
  "scripts": {
    "dev": "vite --host",
    "prebuild": "tsc && vite build --config vite.dist.config.js",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "echo \"No test specified\""
  },
  "devDependencies": {
    "prettier": "^2.7.0",
    "typescript": "^4.5.4",
    "vite": "^2.9.9",
    "sass": "^1.52.3",
    "modern-normalize": "^1.1.0",
    "prismjs": "^1.28.0"
  },
  "dependencies": {}
}
